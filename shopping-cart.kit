<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0 maximum-scale=1.0 user-scalable=0">
	<link rel="stylesheet" href="css/style.css">
	<title>Shopping Cart</title>
</head>
	<body>
		<div class="wrapper">
			<main class="mainPage">
					<!-- @import "html/header.html" -->

					<!-- @import "html/header_menu.html" -->
				<div class="content">
					<!-- ! <NEW ARRIVALS>----------------------------------------------------------- -->
					<div class="newArrivals">
						<div class="container-shop">
							<div class="newArrivals__row">
								<div class="newArrivals__title">New Arrivals</div>
								<!-- ! -->
								<div class="newArrivals__breadcrumbs">
									<ul class="breadcrumb">
										<li class="breadcrumb__list"><a class="breadcrumb__link" href="index.html">Home</a></li>
										<li class="breadcrumb__list"><a class="breadcrumb__link" href="#">Men</a></li>
										<li class="breadcrumb__list active">New Arrivals</li>
									</ul>
								</div>
								<!-- ! -->
							</div>
						</div>
					</div>
					<!-- ! </NEW ARRIVALS>----------------------------------------------------------- -->
					<!-- ! <VIDEO> -->
						<div class="container-shop">
							<div class="player">
								<iframe width="100%" height="640px" src="https://www.youtube.com/embed/LaJi1_F7g10" frameborder="0"
									allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
									allowfullscreen>
								</iframe>
							</div>
						</div>
						
					<!-- ! </VIDEO> -->
					<!-- ! <PRODUCT DETAILS>----------------------------------------------------------- -->
					<div class="container-shop">
						<div class="product-details">

						</div>
						<!-- ! </PRODUCT DETAILS>----------------------------------------------------------- -->
						<!-- ! <SHIPPING ADRESS - COUPON DISCOUNT>-------------------------------------------->
						<div class="shipping-coupon">
							<div class="shipping-coupon__row">
								<!-- ! LEFT-->
								<div class="shipping-coupon__left">
									<div class="shipping-coupon__left-row">
										<div class="shipping-coupon__title">Shipping Adress</div>
										<form action="#">
											<fieldset>
												<div class="bangladesh__select">
													<select name="bangladesh" id="bangladesh">
														<option class="bangladesh-option" value="bangladesh">Bangladesh</option>
														<option class="bangladesh-option" value="germany">Germany</option>
														<option class="bangladesh-option" value="russia">Russia</option>
														<option class="bangladesh-option" value="france">France</option>
														<option class="bangladesh-option" value="choose country">Choose country</option>
													</select>

												</div>
												<input id="country" type="text" name="country" pattern="^[А-Яа-яЁё\s]+$" placeholder="State" required>
												<input id="postcode" type="number" name="postcode" pattern="[0-9]{6}" placeholder="Postcode/Zip" required>
												<div class="quote__btn">
													<a href="#" class="quote__link">get a quote</a>
												</div>
											</fieldset>
										</form>
									</div>
								</div>
								<!-- ! CENTER-->
								<div class="shipping-coupon__center">
									<div class="shipping-coupon__center-row">
										<div class="shipping-coupon__center-title">coupon discount</div>
										<div class="shipping-coupon__center-text">Enter your coupon code if you have one</div>
										<form action="#">
											<fieldset>
												<div class="state">
													<input id="state" type="text" name="state" placeholder="State" required>
												</div>
												<div class="apply-coupon__btn">
													<button type="submit" href="#" class="apply-coupon__link">Apply coupon</button>
												</div>
											</fieldset>
										</form>
									</div>
								</div>
								<!-- ! RIGHT-->
								<div class="shipping-coupon__center">
									<div class="shipping-coupon__center-row">
										<p class="subTotal">Sub total $900</p>
										<p class="grandTotal">GRAND TOTAL <span>$900</span></p>
										<div class="apply-coupon__btn">
											<button type="submit" href="#" class="proceed__link">proceed to checkout</button>
										</div>
									</div>
								</div>
							</div>
						</div>
						<!-- ! </SHIPPING ADRESS - COUPON DISCOUNT>-------------------------------------------- -->
						
				

						


					</div>
					<!-- @import "html/subscribe.html" -->

					<!-- @import "html/brand.html" -->

				</div>
				<!-- @import "html/footer.html" -->
			</main>
		</div>
		
		<!-- Подключение файла JS скриптов -->
		<script src="js/script.js"></script>
	</body>
<html>